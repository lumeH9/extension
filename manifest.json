{
	"manifest_version": 3,
	"name": "Current_url",
	"description": "Gets current tab url",
	"version": "1.0",
	"author": "verkku",
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Analyzing your current url"
	},
	"permissions": ["tabs", "activeTab", "http://192.168.1.87:5000/*", "storage"],
	"background": {
		"service_worker": "background.js"
	}
}