{
	"manifest_version": 3,
	"name": "Current url",
	"description": "sends your current url",
	"version": "1.0",
	"author": "verkku",
	"action": {
		"default_icon": "icon.png",
		"default_title": ""
	},
	"host_permissions": [
		"http://192.168.1.87:5000/*"
	  ],
	  "permissions": [
		"storage", "tabs", "activeTab"
	  ],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	}
}

